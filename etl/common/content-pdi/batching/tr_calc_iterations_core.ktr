    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var i = 0;
var iteration_str = "";
var i_start = min
var i_end = -1;
var first = true;
var break_early = false;

writeToLog("m", "computing batches for min: " + min + " max:" + max);

while(!break_early) {
  //compute next iteration
  i_start = min + i*iteration_size;
  i_end = i_start + iteration_size
 
  //reached the end while still iterating?
  if(i_end > max) {
    i_end = max;
    break_early = true;
  }

  //sanitize string building for the split step
  if(first)
    first = false;
  else {
    iteration_str += ";";   
  }

  iteration_str += i_start + "-" + i_end;
  i++;
}</jsScript_script>
